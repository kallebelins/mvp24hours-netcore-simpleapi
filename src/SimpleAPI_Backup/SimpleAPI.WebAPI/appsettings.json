{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Mvp24Hours": {
    "Startup": {
      "MemoryCache": true,
      "ResponseCaching": false,
      "Profiling": true
    },
    "Persistence": {
      "MaxQtyByQueryPage": 30
    },
    "FileLog": {
      "Path": "",
      "TokenPath": ""
    },
    "SmtpMail": {
      "SmtpServer": "",
      "SmtpUser": "",
      "SmtpPassword": "",
      "SmtpPort": "",
      "SmtpSSL": "",
      "MailTo": "",
      "MailFrom": "",
      "MailFromName": "",
      "ReplyTo": "",
      "ReplyToName": ""
    },
    "Web": {
      "TraceMiddleware": true
    }
  }
}
